package quan_li_ban_hang;

import java.util.Scanner;

public class Ban extends MatHang implements SanPham
{
	private Date date=new Date();
	public Date getDate() {
		return date;
	}
	public void setDate(Date date) {
		this.date=date;
	}
	public Ban() {
		super();
	}
	public Ban(String ma,String ten,String nv,String cc,int sl,double dg,Date date){
		super(ma,ten,nv,cc,sl,dg);
		this.date=date;
	}
	public void input() {
		Scanner kb=new Scanner(System.in);
		System.out.println("Enter Ngay Xuat:");
		date.input();
		super.input();
		
	}
	public void output() {
		super.output();
		System.out.println("Ngay Xuat:");
		date.output();
	}
	@Override
	public double solve() {
		return getSoluong()*getDongia();
	}

	public static void main(String[] args) {
		Ban b=new Ban();
		b.input();
		b.output();
		b.solve();
		System.out.println("Tong tien"+b.solve());

	}

}