package simply_linked_list;

import java.util.LinkedList;

public class MyLinkedList<E> {
	//-------------------------
	// dinh nghia lop Node ben trong lop MyLinkedList
	private class Node{
		private E info;
		private Node next;
		public Node(E info) {
			this.info=info;
			this.next=null;
		}
		public Node(E info,Node next) {
			this.info=info;
			this.next=next;
		}
	}
		//-----------------------------
		// Khai bao thuoc tinh cua lop MyLinkedList
		private Node head,tail;
		private int size;
		
		public MyLinkedList()
		{
			head=tail=null;
			size=0;
		}
		public void addLast(E element)
		{
			Node p = new Node(element);
			// danh sach rong
			if(head==null)
				head=tail=p;
		else
		{
			tail.next=p;
			tail=p;
		}
		size++;
		}
		// tinh tong
		public int sum() {
			int s=0;
			Node q=head;
			while(q!=null)
			{	
				s=s +(Integer)q.info;
				q=q.next;
			}
			return s;
		}
		@Override
		public String toString() {
			String s="";
			Node q = head;
			while(q!=null) {
				s= s+ q.info + " ";
				q=q.next;
			}
			return s;
		}
		// dem co bn gtri chan
		public int coutEven()
	    {
		int k=0;
		Node q= head;
		while(q!=null) {
			if((Integer)q.info%2==0) 
				k++; 
			q=q.next;
		}
		return k;
	    } 
		//ktra nguyen to
		public boolean isPrime(int x) {
			if(x<2)
				return false;
			for(int i=2;i<=Math.sqrt(x);i++)
				if(x%i==0) return false;
			return true;
		}
		// tinh trung binh cong so nguyen to
		public double averagePrime() {
			double s=0; int d=0;
			Node q=head;
			while (q!=null)
			{
				if((isPrime ((Integer)q.info)))
					s=s+(Integer) q.info;
					d++;
					q=q.next;
				}
				return (s/d);		
		}
		//in ra gia tri cua nut cuoi
		public int in() {
			int x=0;
			Node q=head;
			while(q.next==null) {
				x=x+(Integer)q.info;
				q=q.next;
			}
			return x;
			
		}
		//in ra cac gia tri cua nut ke cuoi
		

	public static void main(String[] args) {
		MyLinkedList<Integer> list= new MyLinkedList<Integer>();
		list.addLast(8);
		list.addLast(11);
		list.addLast(9);
		list.addLast(7);
		list.addLast(3);
		System.out.println(list.toString());
		//System.out.println(list.sum());
		//System.out.println(list.coutEven());
		System.out.println(list.averagePrime());
		//System.out.println(list.in());
	
		
	}
}
